        "editor.tabSize": 4,
        "editor.insertSpaces": true
    },
    "[swift]": {
        "editor.tabSize": 4,
        "editor.insertSpaces": true
    },
    "[twig]": {
        "editor.tabSize": 4,
        "editor.insertSpaces": true
    },
    "[blade]": {
        "editor.tabSize": 4,
        "editor.insertSpaces": true
