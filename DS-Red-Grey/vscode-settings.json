    "telemetry.enableTelemetry": false,
    "telemetry.enableCrashReporter": false,
    "workbench.colorTheme": "Tomorrow Night",
    "editor.fontFamily": "'UbuntuMono Nerd Font Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "editor.fontSize": 16,
    "window.zoomLevel": 0,
    "[dockerfile]": {
        "editor.tabSize": 2,
        "editor.insertSpaces": true,
        "editor.quickSuggestions": {
            "strings": true
        }
    },
    "[html]": {
        "editor.tabSize": 4,
        "editor.insertSpaces": true,
        "editor.suggest.insertMode": "replace"
    },
    "[css]": {
        "editor.tabSize": 2,
        "editor.insertSpaces": true,
        "editor.suggest.insertMode": "replace"
    },
    "[json]": {
